{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    },
    {
      "src": "/favicon.ico",
      "dest": "/static/images/favicon.ico"
    },
    {
      "src": "/favicon.png",
      "dest": "/static/images/favicon.png"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  }
}
