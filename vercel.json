{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "rewrites": [
  {
    "source": "/favicon.ico",
    "destination": "/static/images/favicon.ico"
  },
  {
    "source": "/favicon.png",
    "destination": "/static/images/favicon.png"
  }
  ],
  "env": {
    "FLASK_ENV": "production"
  }
}
